{
  "scripts": {
    "lint:base": "eslint --ext .js,.jsx,.ts,.tsx",
    "lint:general": "npm run lint:base -- . --ignore-pattern src/av --ignore-pattern src/node",
    "lint:av": "npm run lint:base -- src/av -c .eslintrc.av.json",
    "lint:node": "npm run lint:base -- src/node -c .eslintrc.node.json",
    "lint": "npm run lint:general && npm run lint:av && npm run lint:node",
    "build:browser": "webpack --config webpack.config.browser.ts",
    "build:electron:av": "webpack --config webpack.config.electron.ts",
    "build:electron:node": "rm -rf lib && tsc -p tsconfig.node.json",
    "build:electron": "npm run build:electron:av && npm run build:electron:node",
    "start:browser": "webpack-dev-server --config webpack.config.browser.ts",
    "start:electron": "npm run build:electron && electron lib",
    "publish": "npm run build:browser && gh-pages -d dist"
  },
  "devDependencies": {
    "@types/color": "^3.0.1",
    "@types/mini-css-extract-plugin": "^0.9.1",
    "@types/react": "^16.9.44",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.9",
    "@types/webpack": "^4.41.21",
    "@types/webpack-merge": "^4.1.5",
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^4.2.0",
    "eslint": "^7.6.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-config-standard-with-typescript": "^18.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.5",
    "eslint-plugin-standard": "^4.0.1",
    "file-loader": "^6.0.0",
    "gh-pages": "^3.1.0",
    "html-webpack-plugin": "^4.3.0",
    "mini-css-extract-plugin": "^0.9.0",
    "null-loader": "^4.0.0",
    "ts-loader": "^8.0.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.1.1"
  },
  "dependencies": {
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "@reduxjs/toolkit": "^1.4.0",
    "color": "^3.1.2",
    "colorthief": "^2.3.2",
    "electron": "^9.1.2",
    "mitt": "^2.1.0",
    "parse-audio-metadata": "^1.0.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.1",
    "ress": "^3.0.0",
    "use-resize-observer": "^6.1.0"
  }
}
