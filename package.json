{
  "scripts": {
    "lint:base": "eslint --ext .js,.jsx,.ts,.tsx",
    "lint:general": "npm run lint:base -- . --ignore-pattern src/av --ignore-pattern src/electron",
    "lint:av": "npm run lint:base -- src/av -c .eslintrc.av.json",
    "lint:electron": "npm run lint:base -- src/electron -c .eslintrc.electron.json",
    "lint": "npm run lint:general && npm run lint:av && npm run lint:electron",
    "build:av": "webpack",
    "build:electron": "rm -rf lib && tsc -p tsconfig.electron.json",
    "build": "npm run build:av && npm run build:electron",
    "start": "npm run build && electron lib"
  },
  "devDependencies": {
    "@types/color": "^3.0.1",
    "@types/mini-css-extract-plugin": "^0.9.1",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.7",
    "@types/react-redux": "^7.1.7",
    "@types/webpack": "^4.41.12",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.5.3",
    "eslint": "^6.8.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-config-standard-with-typescript": "^16.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-standard": "^4.0.1",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "mini-css-extract-plugin": "^0.9.0",
    "ts-loader": "^7.0.2",
    "ts-node": "^8.10.1",
    "typescript": "^3.8.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@fortawesome/react-fontawesome": "^0.1.9",
    "color": "^3.1.2",
    "colorthief": "^2.3.0",
    "electron": "^8.2.5",
    "mitt": "^1.2.0",
    "parse-audio-metadata": "^1.0.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "ress": "^2.0.4",
    "use-resize-observer": "^6.1.0-alpha.3"
  }
}
